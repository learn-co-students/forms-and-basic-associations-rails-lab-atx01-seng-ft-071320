<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name, prompt: true %><br>
  <br>
  <%= f.label :song_note_1 %><br>
  <input id='song_notes_1' name="song[note_contents][]"><br>
  <br>
  <%= f.label :song_note_2 %><br>
  <input id='song_notes_2' name="song[note_contents][]"><br>
  <br>
  <%= f.label :song_note_3 %><br>
  <input id='song-notes_3' name="song[note_contents][]"><br>

  <br>
  <%= f.submit %>
<% end %>


